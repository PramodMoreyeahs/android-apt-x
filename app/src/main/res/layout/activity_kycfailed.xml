<?xml version="1.0" encoding="utf-8"?>


<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

    </data>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/black">

        <ImageView
            android:id="@+id/ivThankYou"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_80sdp"
            android:src="@drawable/layer_1" />


        <TextView
            android:id="@+id/tvTh"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/ivThankYou"
            android:layout_marginVertical="@dimen/_15sdp"
            android:layout_marginStart="@dimen/_10sdp"
            android:layout_marginEnd="@dimen/_10sdp"
            android:fontFamily="@font/lato_regular"

            android:text="@string/kyc_fail"
            android:textColor="@color/white"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/tvThg"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tvTh"
            android:layout_marginVertical="@dimen/_15sdp"
            android:layout_marginStart="@dimen/_10sdp"
            android:layout_marginEnd="@dimen/_10sdp"
            android:fontFamily="@font/lato_regular"

            android:text="@string/kyc_fail2"
            android:textColor="@color/white"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/tvThgk"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tvThg"
            android:layout_marginVertical="@dimen/_15sdp"
            android:layout_marginStart="@dimen/_10sdp"
            android:layout_marginEnd="@dimen/_10sdp"
            android:fontFamily="@font/lato_regular"

            android:text="@string/kyc_fail3"
            android:textColor="@color/white"
            android:textSize="14sp" />


        <LinearLayout
            android:id="@+id/maillyt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tvThgk"
            android:gravity="center"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/mailicon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:layout_marginStart="@dimen/_10sdp"
                android:src="@drawable/ic_email_iconred">

            </ImageView>

            <TextView
                android:id="@+id/mailtext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginVertical="@dimen/_15sdp"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_10sdp"
                android:fontFamily="@font/lato_regular"
                android:text="Email:"

                android:textColor="@color/white"

                android:textSize="14sp" />

            <TextView
                android:id="@+id/mailtext2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginVertical="@dimen/_15sdp"
                android:layout_marginEnd="@dimen/_10sdp"
                android:fontFamily="@font/lato_regular"

                android:text="Support@aptpay.com"

                android:textColor="@color/white"

                android:textSize="14sp" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/mblelyt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/maillyt"
            android:gravity="center"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/callicon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:layout_marginStart="@dimen/_10sdp"
                android:src="@drawable/ic_call_red">

            </ImageView>

            <TextView
                android:id="@+id/calltext"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginVertical="@dimen/_15sdp"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_10sdp"
                android:fontFamily="@font/lato_regular"
                android:text="Phone:"

                android:textColor="@color/white"

                android:textSize="14sp" />

            <TextView
                android:id="@+id/calltext2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginVertical="@dimen/_15sdp"
                android:layout_marginEnd="@dimen/_10sdp"
                android:fontFamily="@font/lato_regular"

                android:text="1.866.303.3955"

                android:textColor="@color/white"

                android:textSize="14sp" />

        </LinearLayout>

        <TextView
            android:id="@+id/tvContinue"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_60sdp"
            android:layout_below="@+id/mblelyt"
            android:layout_marginHorizontal="@dimen/_20sdp"
            android:background="@drawable/tv_btn_bg"
            android:fontFamily="@font/montserrat_medium"
            android:gravity="center"
            android:paddingVertical="@dimen/_13sdp"
            android:text="Back to Home"
            android:textColor="@color/white"
            android:visibility="visible" />
    </RelativeLayout>
</layout>
